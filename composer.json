{
    "name": "xttribute/xttribute",
    "description": "XML to DTO, nicely!",
    "license": ["MIT"],
    "require-dev": {
        "pestphp/pest": "^1.21",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Fixtures\\": "tests/Fixtures"
        }
    },
    "autoload": {
        "psr-4": {
            "Xttribute\\Xttribute\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --ci",
        "phpcs": "vendor/bin/phpcs --standard=PSR12 src tests"
    },
    "require": {
        "ext-dom": "*"
    }
}
